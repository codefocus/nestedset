{
    "name": "codefocus/nestedset",
    "description": "Nested set implementation for Eloquent models in Laravel.",
    "keywords": [
        "codefocus",
        "nestedset",
        "nested set",
        "hierarchy",
        "hierarchical",
        "tree"
    ],
    "homepage": "https://github.com/codefocus/nestedset",
    "license": "MIT",
    "authors": [
        {
            "name": "Menno van Ens",
            "email": "info@codefocus.ca",
            "homepage": "http://www.codefocus.ca/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Codefocus\\NestedSet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codefocus\\NestedSet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.1-dev"
        }
    }
}
